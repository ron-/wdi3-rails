<table>
  <tr>
    <td><%= @album.name %></td>
    <td><%= image_tag @album.image %></td>
  </tr>
</table>

<h5>Album Songs</h5>
<table>
  <% @album.songs.each do |song| %>
  <tr>
    <td><%= song.name %></td>
    <td><%= audio_tag(song.filename, :controls => true) %></td>
  </tr>
  <% end %>
</table>



